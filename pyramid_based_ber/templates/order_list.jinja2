{% extends "index.jinja2" %}

{% block content %}
    <h1>Данные для доставки</h1>
    <table border="1">
    <tr>
      <td>Номер</td>
      <td>Дата</td>
      <td>Имя</td>
      <td>Телефон</td>
      <td>Адрес</td>
      <td>Номер бургера</td>
      </tr>
        {% for order_info in order_infos %}
        <tr>
          <td>{{ order_info.id }}</td>
          <td>{{ order_info.time }}</td>
          <td>{{ order_info.name }}</td>
          <td>{{ order_info.phone }}</td>
          <td>{{ order_info.address }}</td>
            <td><a href="/burgers"> {{ order_info.burger }} </a></td>
      </tr>
        {% endfor %}
    </table>
{% endblock content %}